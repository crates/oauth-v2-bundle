{
    "name": "keboola\/oauthv2bundle",
    "type": "symfony-bundle",
    "description": "OAuth V2 bundle",
    "keywords": [],
    "authors": [],
    "repositories": [],
    "require": {
        "defuse/php-encryption": "^1.2",
        "keboola/syrup": "~2.3",
        "keboola/kbc-manage-api-php-client": "^1.3",
        "keboola/oauth": "~0.1.0",
        "keboola/php-utils": "^0.1.20",
        "guzzlehttp/guzzle": "^6.1",
        "keboola/syrup-php-client": "^2.1",
        "facebook/graph-sdk": "~5.0"
    },
    "require-dev": {
        "phpunit\/phpunit": "3.7.*"
    },
    "scripts": {
        "post-install-cmd": [
            "Keboola\\OAuthV2Bundle\\DeploymentHandler\\ScriptHandler::getParameters",
            "Keboola\\OAuthV2Bundle\\DeploymentHandler\\ScriptHandler::getSharedParameters",            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap"
        ],
        "post-update-cmd": [
            "Keboola\\OAuthV2Bundle\\DeploymentHandler\\ScriptHandler::getParameters",
            "Keboola\\OAuthV2Bundle\\DeploymentHandler\\ScriptHandler::getSharedParameters",            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache"
        ]
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-0": {
            "Keboola\\OAuthV2Bundle": ""
        }
    },
    "target-dir": "Keboola\/OAuthV2Bundle",
    "extra": {
        "symfony-app-dir": "vendor\/keboola\/syrup\/app",
        "symfony-web-dir": "vendor\/keboola\/syrup\/web",
        "syrup-app-name": "oauth-v2"
    }
}
